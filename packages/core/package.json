{
  "name": "@jaystack/sls-core",
  "version": "0.0.0",
  "description": "AWS Serverless core utilities",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile:cjs": "tsc -p tsconfig.build.json",
    "compile:esm": "tsc -p tsconfig.build.esm.json",
    "compile": "npm run compile:cjs && npm run compile:esm",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/jest": "^26.0.9",
    "jest": "^26.2.2",
    "ts-jest": "^26.1.4"
  }
}
