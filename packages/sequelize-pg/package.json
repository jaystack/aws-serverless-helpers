    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile": "npm run compile:cjs && npm run compile:esm",
    "prepublishOnly": "npm run build",
    "test": "jest -b"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "aws-sdk": "^2.771.0",
    "jest": "^26.5.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@jaystack/sls-core": "^0.0.1-alpha.4",
    "debug": "^4.2.0",
    "sequelize": "^6.3.5"
