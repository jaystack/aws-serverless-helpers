{
  "name": "@jaystack/sls-sequelize-pg",
  "license": "MIT",
  "version": "0.0.1-alpha.6",
  "description": "AWS Serverless Sequelize PostgresDB utilities",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jaystack/aws-serverless-helpers.git",
    "directory": "packages/sequelize-pg"
  },
  "bugs": {
    "url": "https://github.com/jaystack/aws-serverless-helpers/issues"
  },
  "homepage": "https://github.com/jaystack/aws-serverless-helpers#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
